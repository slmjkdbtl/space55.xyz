{
	"compilerOptions": {
		"target": "es2021",
		"lib": ["esnext"],
		"module": "esnext",
		"moduleResolution": "bundler",
		"types": [
			"@cloudflare/workers-types/2023-07-01",
			"@types/node"
		],
		"resolveJsonModule": true,
		"noEmit": true,
		"isolatedModules": true,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
